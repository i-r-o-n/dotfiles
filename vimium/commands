unmapAll

# editing modes
map s enterInsertMode
map gs focusInput
map t enterVisualMode
map T enterVisualLineMode

# page navigation
map e scrollUp
map d scrollDown
map c scrollLeft
map a scrollRight

map gg scrollToTop
mag G scrollToBottom

map ku goUp
map kU goToRoot

# page links
map n LinkHints.activateMode
map N LinkHints.activateModeToOpenInNewTab
map bn LinkHints.activateModeToCopyLinkUrl
map gn LinkHints.activateModeToOpenInNewForegroundTab

# marks
map z Marks.activateCreateMode
map ' Marks.activateGotoMode

# tabs movement
map D nextTab
map E previousTab
map , visitPreviousTab
map ge moveTabLeft
map gd moveTabRight
map g<space> moveTabToNewWindow
map <space> Vomnibar.activateTabSelection

# tab management
map o createTab
map O duplicateTab
map x removeTab
map X restoreTab

# tab control
map r reload
# map kc goBack
# map ka goForward
map C goBack
map A goForward

# url management
map gu Vomnibar.activateEditUrl
map gU Vomnibar.activateEditUrlInNewTab
map b copyCurrentUrl
map m openCopiedUrlInCurrentTab
map M openCopiedUrlInNewTab

# find
map i enterFindMode
map p performFind
map P performBackwardsFind

# omnibar
map r Vomnibar.activate
map R Vomnibar.activateInNewTab


# misc
map ? showHelp
map gs toggleViewSource
